@using BetTeamsBattle.Data.Model.Entities
@model IEnumerable<Battle>
@{
    Layout = MVC.Shared.Views.OneColumn._OneColumnLayout;
}
@section Title {
    Battles
}
@foreach (var battle in Model)
{
    <text> Id @battle.Id
    <br />
    Start date @battle.StartDate
    <br />
    End date @battle.EndDate
    <br />
    Battle type @battle.BattleType
    <br />
    Budget @battle.Budget
    <br />
    <br />
    <br />
    </text>
}
