@using BetTeamsBattle.Frontend.Areas.Admin.Models.Teams
@model CreateProTeamViewModel
@{
    Layout = MVC.Shared.Views.OneColumn._OneColumnLayout;
}
@section Title {
    Create pro team
}
@using (Html.BeginForm(MVC.Admin.AdminTeams.CreateProTeam(), FormMethod.Post, new { @class = "form" }))
{
    @Forms.TextBox(Html, m => m.Title, "Title")
    @Forms.TextBox(Html, m => m.Description, "Description")
    @Forms.TextBox(Html, m => m.Url, "Url")

    @Forms.SubmitButton("Create pro team")
}